{
    "compilerOptions": {
        "target": "ES2024",
        "checkJs": true,
        "allowJs": true,
        "strict": true,
        "moduleResolution": "nodenext",
        "module": "nodenext",
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "noEmit": true,
        "types": ["vite/client"]
    },
    "include": ["src/**/*.js"],
    "exclude": ["**/**/bundle.js"]
}
